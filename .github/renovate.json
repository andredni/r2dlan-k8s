{
    "extends": [
      "config:base"
    ],
    "dependencyDashboard": false,
    "timezone": "Europe/Berlin",
    "labels": ["helm", "dependencies"],
    "prHourlyLimit": 5,
    "prConcurrentLimit": 10,
    "packageRules": [
      {
        "matchManagers": ["helmfile"],
        "enabled": true,
        "groupName": "helm updates",
        "schedule": ["before 5am on monday"],
        "automerge": false,
        "rebaseWhen": "conflicted"
      }
    ]
  }
  